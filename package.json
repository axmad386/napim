{
  "name": "napim",
  "version": "4.0.0-beta2",
  "description": "Node Api Maker build with polka",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "watch": "nodemon --es-module-specifier-resolution=node dist/index.js",
    "watch-test": "nodemon --es-module-specifier-resolution=node dist_test/index.js",
    "build": "rm -rf dist && tsc",
    "build-test": "rm -rf dist_test & cd test && tsc && cd ..",
    "dev": "run-s build build-test watch watch-test",
    "start": "node --experimental-modules --es-module-specifier-resolution=node dist_test/index.js",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axmad386/napim.git"
  },
  "keywords": [
    "polka",
    "nodejs",
    "api",
    "knex"
  ],
  "author": "Akhmad Salafudin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/axmad386/napim/issues"
  },
  "homepage": "https://github.com/axmad386/napim#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "node-input-validator": "next",
    "polka": "next",
    "simple-node-logger": "^18.12.24"
  },
  "devDependencies": {
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "auto-changelog": "^2.2.1",
    "bcryptjs": "^2.4.3",
    "knex": "^0.21.15",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "pg": "^8.5.1",
    "sqlite3": "^5.0.2",
    "typescript": "^4.0.3"
  },
  "type": "module"
}
